	   <class>comp.is.model.project.entity.RatesheetEntity</class>
	   <class>comp.is.model.project.entity.RoleEntity</class>
	   <class>comp.is.model.project.entity.TimesheetEntity</class>
	   <class>comp.is.model.project.entity.TimesheetentryEntity</class>
	   <class>comp.is.model.project.entity.TimesheetweekEntity</class>
	   <class>comp.is.model.project.entity.WorkassignmentEntity</class>
	   <class>comp.is.model.project.entity.WorkpackageEntity</class>
	   <class>comp.is.model.project.key.WorkpackagePK</class>
	   <class>comp.is.model.project.entity.WorkpackagestatusreportEntity</class>
